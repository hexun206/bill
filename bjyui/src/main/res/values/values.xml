    <color name="bjplayer_controller_bg_99">#99000000</color>
    <declare-styleable name="BJPlayerView">
        <attr format="reference" name="top_controller"/>
        <attr format="reference" name="center_controller"/>
        <attr format="reference" name="bottom_controller"/>

        <attr format="enum" name="bjplayer_type">
            <enum name="ijk_surface_view" value="2"/>
            <enum name="ijk_texture_view" value="3"/>
        </attr>
   </declare-styleable>
    <declare-styleable name="BJVideoView">
        <attr format="enum" name="aspect_ratio">
            <enum name="fit_parent" value="0"/>
            <enum name="fill_parent" value="1"/>
            <enum name="wrap_content" value="2"/>
            <enum name="match_parent" value="3"/>
            <enum name="fit_parent_16_9" value="4"/>
            <enum name="fit_parent_4_3" value="5"/>
        </attr>

        <attr format="enum" name="player_type">
            <enum name="android" value="1"/>
            <enum name="ijk_surface_view" value="2"/>
            <enum name="ijk_texture_view" value="3"/>
        </attr>

        <attr format="boolean" name="auto_play"/>
    </declare-styleable>
    <item name="water_mark_iv_id" type="id"/>
