                        android:text="请稍后点击刷新"
                        android:textColor="@color/gray004"
                        app:layout_textSizePercent="3.94%w"
                        android:layout_height="wrap_content"/>
                </com.zhy.android.percent.support.PercentRelativeLayout>
            </com.zhy.android.percent.support.PercentRelativeLayout>


            <com.huatu.teacheronline.widget.rollviewpager.RollPagerView
                android:id="@+id/img_mian_adpic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
