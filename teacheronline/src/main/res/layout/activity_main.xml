                        android:text="请稍后点击刷新"
                        android:textColor="@color/white"
                        app:layout_textSizePercent="3.94%w"
                        android:layout_height="wrap_content"/>
                </com.zhy.android.percent.support.PercentRelativeLayout>

                <ImageView
                    android:id="@+id/img_play_main"
                    android:layout_width="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    app:layout_marginLeftPercent="3.5%w"
                    app:layout_marginBottomPercent="3.5%w"
                    android:background="@drawable/ic_bt_play"
                    android:layout_height="wrap_content"/>
            </com.zhy.android.percent.support.PercentRelativeLayout>

            <com.zhy.android.percent.support.PercentRelativeLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                app:layout_marginLeftPercent="2.5%w"
                app:layout_marginRightPercent="2.5%w"
                app:layout_heightPercent="22.4%w"
                android:layout_height="wrap_content">

                <ListView
                    android:id="@+id/listview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:cacheColorHint="#00000000"
