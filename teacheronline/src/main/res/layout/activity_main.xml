                android:visibility="visible"
                app:layout_heightPercent="23.89%w"
                app:layout_marginTopPercent="5.81%w"
                app:rollviewpager_play_delay="3000"/>
        </com.zhy.android.percent.support.PercentLinearLayout>
        </ScrollView>
