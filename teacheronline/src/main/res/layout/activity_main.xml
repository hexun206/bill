                    <TextView
                        android:id="@+id/tv_refresh_dire"
                        android:layout_toRightOf="@+id/img_refresh_dire"
                        android:layout_width="wrap_content"
                        app:layout_marginLeftPercent="1.5%w"
                        app:layout_marginTopPercent="8%w"
                        android:text="直播获取失败"
                        android:textColor="@color/gray004"
                        app:layout_textSizePercent="5.94%w"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:layout_toRightOf="@+id/img_refresh_dire"
