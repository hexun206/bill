        <!--android:layout_alignParentBottom="true"-->
        <!--android:background="@color/gray004"/>-->
</com.zhy.android.percent.support.PercentRelativeLayout>

     <com.zhy.android.percent.support.PercentLinearLayout
         android:layout_height="wrap_content"
         android:layout_width="match_parent"
         android:background="@color/white"
         android:paddingBottom="8dp"
         android:orientation="horizontal"
         >
     <TextView
         android:id="@+id/tv_time"
         android:layout_width="wrap_content"
         android:text=""
         app:layout_marginLeftPercent="9.72%w"
         app:layout_textSizePercent="2.78%w"
         android:layout_height="wrap_content" />
         <TextView
             android:id="@+id/tv_message_type"
             android:layout_width="wrap_content"
             android:text=""
             app:layout_marginLeftPercent="5%w"
             app:layout_textSizePercent="2.78%w"
             android:layout_height="wrap_content" />

     </com.zhy.android.percent.support.PercentLinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/width_1"
            app:layout_marginRightPercent="3.19%w"
            app:layout_marginLeftPercent="3.19%w"
            android:background="@color/gray004"/>
        <com.zhy.android.percent.support.PercentRelativeLayout
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_width="match_parent">

            <TextView
                android:id="@+id/tv_prompt"
                android:layout_width="wrap_content"
                android:text=""
                app:layout_marginRightPercent="3.19%w"
                app:layout_marginLeftPercent="3.19%w"
                app:layout_marginTopPercent="4.44%w"
                android:textColor="@color/black007"
                app:layout_marginBottomPercent="7.44%w"
                app:layout_textSizePercent="3.89%w"
                android:layout_height="wrap_content" />
            <com.zhy.android.percent.support.PercentRelativeLayout
                android:layout_width="match_parent"
                app:layout_heightPercent="10.4%w"
                android:layout_alignParentBottom="true"
                android:layout_height="wrap_content">
                <TextView
                android:id="@+id/tv_num"
                android:layout_width="wrap_content"
                android:text="已有0人确定"
                android:layout_centerVertical="true"
                app:layout_marginLeftPercent="3.4%w"
                android:textColor="@color/gray013"
                app:layout_textSizePercent="3.33%w"
                android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/tv_leave"
                    android:layout_width="wrap_content"
                    android:text="添加留言"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    app:layout_marginRightPercent="3.4%w"
                    android:drawablePadding="2dp"
                    android:drawableLeft="@drawable/me_add"
                    android:textColor="@color/green013"
                    app:layout_textSizePercent="3.33%w"
                    android:layout_height="wrap_content" />
            </com.zhy.android.percent.support.PercentRelativeLayout>

        </com.zhy.android.percent.support.PercentRelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/width_1"
            android:visibility="gone"
            android:background="@color/gray004"/>

        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="match_parent"
            android:background="@color/white"
            app:layout_marginTopPercent="3.05%w"
            android:orientation="vertical"
            android:layout_height="wrap_content">

            <com.huatu.teacheronline.widget.CustomListView
                android:id="@+id/lisiview"
                android:layout_width="match_parent"
                android:dividerHeight="10dp"
                android:divider="@color/white008"
                android:layout_height="wrap_content">

            </com.huatu.teacheronline.widget.CustomListView>

            <!--<View-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"></View>-->

        </com.zhy.android.percent.support.PercentLinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/white008"
            android:layout_height="50dp"></LinearLayout>

    </com.zhy.android.percent.support.PercentLinearLayout>
</com.zhy.android.percent.support.PercentRelativeLayout>
