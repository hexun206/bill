<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:paddingRight="12dp"
    app:layout_marginLeftPercent="3.9%w">

    <TextView
        android:id="@+id/tv_num_subjective"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_gravity="center"
        android:background="@drawable/no_zgt"
        android:gravity="center"
        android:text="14"
        app:layout_marginLeftPercent="3.9%w"/>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginRight="14dp"
            android:gravity="right"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_score_subjective"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12"
                android:textColor="#0ab28b"
                android:textSize="22dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="/"
                android:textSize="14dp"/>

            <TextView
                android:id="@+id/tv_total_subjective"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="30"
                android:textSize="14dp"/>
        </com.zhy.android.percent.support.PercentLinearLayout>

        <ProgressBar
            android:id="@+id/pb_subjective"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="5.5dp"
            android:layout_below="@+id/linearLayout"
            android:progress="70"
            android:progressDrawable="@drawable/laryer_progressbar_style2"
            app:layout_marginLeftPercent="2.78%w"
            app:layout_marginRightPercent="2.78%w"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="12"
            android:textSize="20sp"
            android:visibility="invisible"/>
    </com.zhy.android.percent.support.PercentLinearLayout>
