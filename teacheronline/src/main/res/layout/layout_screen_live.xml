<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#dbdbdb"/>

        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/ll_test_categories"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_marginTopPercent="3.61%w">

            <com.zhy.android.percent.support.PercentRelativeLayout
                android:id="@+id/rl_test_categories"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >


                <TextView
                    android:id="@+id/tv_test_categories"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/test_categories"
                    android:textColor="@color/black"
                    app:layout_marginLeftPercent="3.33%w"
                    app:layout_textSizePercent="5.17%w"
                    />

            </com.zhy.android.percent.support.PercentRelativeLayout>

            <com.huatu.teacheronline.widget.RadioGroupEx
                android:id="@+id/gv_test_categories"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                app:layout_marginTopPercent="3.33%w"
                app:layout_paddingLeftPercent="1.5%w"/>
        </com.zhy.android.percent.support.PercentLinearLayout>

        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/ll_interview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone"
            app:layout_marginTopPercent="3.61%w">

            <com.zhy.android.percent.support.PercentRelativeLayout
                android:id="@+id/rl_interview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/tv_interview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/interview"
                    android:textColor="@color/black"
                    app:layout_marginLeftPercent="3.33%w"
                    app:layout_textSizePercent="5.17%w"/>

            </com.zhy.android.percent.support.PercentRelativeLayout>

            <com.huatu.teacheronline.widget.RadioGroupEx
                android:id="@+id/gv_interview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                app:layout_marginTopPercent="3.33%w"
                app:layout_paddingLeftPercent="1.5%w"/>


        </com.zhy.android.percent.support.PercentLinearLayout>

        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/ll_learning_period"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone"
            app:layout_marginTopPercent="3.61%w">

            <com.zhy.android.percent.support.PercentRelativeLayout
                android:id="@+id/rl_learning_period"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/tv_learning_period"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/learning_period"
                    android:textColor="@color/black"
                    app:layout_marginLeftPercent="3.33%w"
                    app:layout_textSizePercent="5.17%w"/>

            </com.zhy.android.percent.support.PercentRelativeLayout>

            <com.huatu.teacheronline.widget.RadioGroupEx
                android:id="@+id/gv_learning_period"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                app:layout_marginTopPercent="3.33%w"
                app:layout_paddingLeftPercent="1.5%w"/>


        </com.zhy.android.percent.support.PercentLinearLayout>

        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/ll_class_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone"
            app:layout_marginTopPercent="3.61%w">

            <com.zhy.android.percent.support.PercentRelativeLayout
                android:id="@+id/rl_class_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/tv_class_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/class_type"
                    android:textColor="@color/black"
                    app:layout_marginLeftPercent="3.33%w"
                    app:layout_textSizePercent="5.17%w"/>

            </com.zhy.android.percent.support.PercentRelativeLayout>

            <com.huatu.teacheronline.widget.RadioGroupEx
                android:id="@+id/gv_class_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                app:layout_marginTopPercent="3.33%w"
                app:layout_paddingLeftPercent="1.5%w"/>


        </com.zhy.android.percent.support.PercentLinearLayout>


        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/ll_subjects"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone"
            app:layout_marginTopPercent="3.61%w">

            <com.zhy.android.percent.support.PercentRelativeLayout
                android:id="@+id/rl_subjects"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/tv_subjects"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/subjects"
                    android:textColor="@color/black"
                    app:layout_marginLeftPercent="3.33%w"
                    app:layout_textSizePercent="5.17%w"/>

            </com.zhy.android.percent.support.PercentRelativeLayout>

            <com.huatu.teacheronline.widget.RadioGroupEx
                android:id="@+id/gv_subjects"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                app:layout_marginTopPercent="3.33%w"
                app:layout_paddingLeftPercent="1.5%w"/>

        </com.zhy.android.percent.support.PercentLinearLayout>
    </com.zhy.android.percent.support.PercentLinearLayout>


</com.zhy.android.percent.support.PercentRelativeLayout>