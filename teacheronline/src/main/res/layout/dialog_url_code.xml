<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <com.zhy.android.percent.support.PercentLinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_heightPercent="14.03%w">
        <com.zhy.android.percent.support.PercentRelativeLayout
            android:id="@+id/rl_titleContainer_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white003"
            app:layout_heightPercent="13.89%w">
            <com.zhy.android.percent.support.PercentRelativeLayout
                android:id="@+id/rl_dialog_canle"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                app:layout_widthPercent="13.89%w">
                <ImageView
                    android:id="@+id/ib_main_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:scaleType="fitCenter"
                    android:src="@drawable/back_direct"/>
            </com.zhy.android.percent.support.PercentRelativeLayout>

            <TextView
                android:id="@+id/tv_main_title"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_centerInParent="true"
                android:ellipsize="end"
                android:lines="1"
                android:gravity="center_vertical"
                android:text=""
                android:textColor="@color/black"
                app:layout_marginLeftPercent="13.66%w"
                app:layout_marginRightPercent="13.66%w"
                app:layout_textSizePercent="5%w"/>
        </com.zhy.android.percent.support.PercentRelativeLayout>
        <View
            android:id="@+id/view_line"
            android:layout_width="match_parent"
            android:layout_height="@dimen/width_1"
            android:background="@color/gray004"/>
    </com.zhy.android.percent.support.PercentLinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <WebView
            android:id="@+id/cwq_web"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></WebView>
    </RelativeLayout>
</com.zhy.android.percent.support.PercentLinearLayout>
